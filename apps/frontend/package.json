{
  "name": "@social/frontend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Frontend Vue application for social media",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "pinia": "^2.3.0",
    "primevue": "^4.3.0",
    "primeicons": "^7.0.0",
    "@primevue/themes": "^4.5.0",
    "@vueuse/core": "^11.3.0",
    "axios": "^1.7.9",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "vite": "^6.0.5",
    "typescript": "^5.6.0",
    "vue-tsc": "^2.2.0",
    "vitest": "^2.1.8",
    "@vitest/coverage-v8": "^2.1.8",
    "@vue/test-utils": "^2.4.6",
    "jsdom": "^25.0.1",
    "tailwindcss": "^3.4.17",
    "postcss": "^8.4.49",
    "autoprefixer": "^10.4.20",
    "@tailwindcss/forms": "^0.5.9",
    "eslint": "^9.0.0",
    "eslint-plugin-vue": "^9.31.0",
    "@typescript-eslint/parser": "^8.19.1",
    "@typescript-eslint/eslint-plugin": "^8.19.1"
  }
